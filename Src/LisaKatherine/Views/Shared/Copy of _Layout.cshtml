<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/css/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.4.4.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.lkb.js")" type="text/javascript"></script>
    <link href='http://fonts.googleapis.com/css?family=Reenie+Beanie|Droid+Sans' rel='stylesheet' type='text/css' />
</head>
<body>
    <div id="header">
        <div id="logindisplay">
            @Html.Partial("_LogOnPartial")
        </div>
        <div id="menucontainer">
            <ul id="menu">
                <li class="link1">@Html.ActionLink("Home", "Index", "Home")</li>
                <li class="link2">@Html.ActionLink("Blog", "", "Blog")</li>
                <li class="link3">@Html.ActionLink("Photos", "", "Photos")</li>
                <li class="link4">@Html.ActionLink("Work", "", "Work")</li>
                <li class="link5">@Html.ActionLink("Contact", "Contact", "Home")</li>
            </ul>
        </div>
    </div>
    <div id="main">
        <div class="head">
        </div>
        <div id="container3" class="body">
            <div id="container2">
                <div id="col2">
                    <h1>
                        Lisa Katherine </h1>
                    @RenderBody()</div>
                <div id="col3">
                    @Html.Partial("_TwitterFeed")
                    @{Html.RenderAction("RenderBlogFeed", "Blog");}
                </div>
            </div>
        </div>
    </div>
    <div id="footer">
        @{Html.RenderAction("RenderBlogJs", "Blog");}
    </div>
</body>
</html>
