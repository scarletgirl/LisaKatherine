@model IEnumerable<LisaKatherine.Interface.IPhoto>
@foreach (var item in Model)
{           
    <div id="FlickrImage" class="@item.Orientation">
        <a href="@item.Link" title="@item.Title" target="flickr" alt="@item.Title">
            <img src="@item.MediumUrl" title="@item.Title" alt="@item.Title" />
            <div class="title">@item.Title</div>
        </a>
    </div>
}
