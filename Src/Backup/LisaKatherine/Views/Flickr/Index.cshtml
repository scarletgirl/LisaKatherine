@using LisaKatherine.Models;
@model IEnumerable<LisaKatherine.Models.FlickrSets>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<h1>
    Flickr</h1>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Id
        </th>
        <th>
            Name
        </th>
        <th>
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.SetId
            </td>
            <td>
                @item.SetName
            </td>
 
            <td><a href='@Settings.FlickrSetUrl(item.SetId)' target="_blank">Link</a> |
                @Html.ActionLink("Edit", "Edit", new { id = item.SetId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.SetId })
            </td>
        </tr>
    }
</table>
